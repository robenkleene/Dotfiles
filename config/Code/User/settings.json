{
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.colorCustomizations": {
    "editor.foreground": "#aabbcc",
    "editor.selectionBackground": "#284667",
    "editor.selectionHighlightBackground": "#444",
    "editor.findMatchBackground": "#284667",
    "editor.findMatchHighlightBackground": "#444",
    "list.activeSelectionBackground": "#333",
    "list.activeSelectionForeground": "#aabbcc",
    "list.focusBackground": "#444",
    "list.focusForeground": "#aabbcc",
    "list.inactiveFocusBackground": "#333",
    "list.inactiveFocusForeground": "#aabbcc",
    "list.inactiveSelectionBackground": "#333",
    "list.inactiveSelectionForeground": "#aabbcc",
    "sideBar.foreground": "#888"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "punctuation.definition.heading.markdown, punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.metadata.markdown, markup.underline.link.markdown, punctuation.definition.bold.markdown, markup.underline.link.image.markdown, punctuation.definition.italic.markdown, punctuation.definition.raw.markdown",
        "settings": {
          "foreground": "#777"
        }
      },
      {
        "scope": "punctuation.definition.list.begin.markdown, markup.list, text",
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "scope": "markup.raw.block.markdown, markup.inline.raw.string.markdown, markup.quote.markdown",
        "settings": {
          "foreground": "#888"
        }
      },
      {
        "scope": "entity.name.section.markdown, markup.italic.markdown, markup.bold.markdown",
        "settings": {
          "foreground": "#fff"
        }
      }
    ]
  },
  "update.showReleaseNotes": false,
  "workbench.colorTheme": "SynthWave '84",
  "workbench.activityBar.visible": true,
  "C_Cpp.updateChannel": "Insiders",
  "editor.minimap.enabled": false,
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "search.useGlobalIgnoreFiles": true,
  "debug.onTaskErrors": "showErrors",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "bracket-pair-colorizer-2.excludedLanguages": ["markdown"],
  "c-cpp-flylint.cppcheck.enable": false,
  "c-cpp-flylint.flexelint.enable": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabCompletion": "onlySnippets",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.cursorBlinking": "phase",
  "workbench.editor.closeOnFileDelete": true,
  "extensions.ignoreRecommendations": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "bracket-pair-colorizer-2.showVerticalScopeLine": false,
  "breadcrumbs.enabled": true,
  "window.zoomLevel": 0,
  "editor.showFoldingControls": "always",
  "window.openFilesInNewWindow": "default",
  "explorer.compactFolders": false,
  "editor.find.autoFindInSelection": "multiline",
  "gitlens.currentLine.enabled": false,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "window.newWindowDimensions": "inherit",
  "explorer.sortOrder": "mixed",
  "markdownlint.config": {
    // This config is for `markdownlint` CLI, VS Code is configured in settings
    "default": true,
    // Four spaces for nested lists
    "MD007": { "indent": 4 },
    // Allow trailing whitespace (this is a good rule, but it's too annoying
    // when typing)
    "MD009": false,
    // Allow hard tabs, e.g., in code blocks
    "MD010": false,
    // Allow long lines
    "MD013": false,
    // Allow duplicate headings
    "MD024": false,
    // Allow punctuation at the end of headings (e.g., `?`)
    "MD026": false,
    // Don't require blank lines around fenced code blocks, hierarchical code
    // blocks in lists look better without blank lines around them.
    "MD031": false,
    // Disable forcing blank lines around lists, this is just annoying when
    // creating a list
    "MD032": false,
    // Allow fenced code blocks without a language specified
    "MD040": false,
    // Don't force first line header
    "MD041": false,
    // Allow either fenced code blocks or indented code blocks
    "MD046": false,
    // Disable blank line ending file, this is a good rule, but annoying to
    // enforce while editing
    "MD047": false
  },
  "files.associations": {
    "*.omnifocusjs": "javascript",
    "*.omnijs": "javascript"
  },
  "prettier.config": {
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "write-good.write-good-config": {
    "so": false,
    "adverb": false,
    "tooWordy": false,
    "passive": false,
    "thereIs": false,
    "weasel": false
  },
