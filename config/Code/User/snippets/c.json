{
  "utemplate": {
    "prefix": "utemplate",
    "body": "#include <stdio.h>\n\nint main() {\n    printf(\"hello, world\\n\");\n}",
    "description": "C template"
  },
  "uprintf_var": {
    "prefix": "uprintf_var",
    "body": "uprintf(\"$1 = %${2:i}\\n\", ${1:result});",
    "description": "Print variable"
  },
  "ufor": {
    "prefix": "ufor",
    "body": "for (int ${1:i} = 0; $1 < ${2:10}; $1++) {\n    $0\n}",
    "description": "For loop"
  },
  "uwhile_getchar": {
    "prefix": "uwhile_getchar",
    "body": "while ((c = getchar()) != EOF) {\n\t$0\n}",
    "description": "While character loop"
  }
}
