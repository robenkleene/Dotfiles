{
  "printlnv": {
    "scope": "rust",
    "prefix": "printlnv",
    "body": "println!(\"${1:foo} = {}\", $1);",
    "description": "Print variable"
  },
  "printlnvd": {
    "scope": "rust",
    "prefix": "printlnvd",
    "body": "println!(\"${1:foo} = {:?}\", $1);",
    "description": "Print variable debug"
  },
  "template": {
    "prefix": "template",
    "body": "fn main() {\n    ${1:println!(\"Hello, world!\");}\n}\n",
    "description": "Template"
  },
  "matchopt": {
    "prefix": "matchopt",
    "body": "match ${1:result} {\n  Some($1) => $1,\n  None => ${2:println!(\"No value\")},\n}",
    "description": "matchopt"
  }
}

