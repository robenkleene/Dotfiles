{
  "name": "robenkleene",
  "displayName": "robenkleene",
  "description": "Roben Kleene's personal customizations",
  "publisher": "robenkleene",
  "version": "0.0.1",
  "repository": {
      "type": "git",
      "url": "https://github.com/robenkleene/vscode-robenkleene"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "extensionKind": ["ui"],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "robenkleene.copyGrep",
        "title": "Copy grep"
      },
      {
        "command": "robenkleene.copyGrepMarkdown",
        "title": "Copy grep Markdown"
      },
      {
        "command": "robenkleene.diffGotoSource",
        "title": "Go to Source"
      }
    ],
    "keybindings": [
      {
        "command": "robenkleene.diffGotoSource",
        "key": "f12",
        "when": "editorLangId == 'diff'"
      }
    ],
    "menus": {
      "editor/context/copy": [
        {
          "command": "robenkleene.copyGrep"
        },
        {
          "command": "robenkleene.copyGrepMarkdown"
        }
      ],
      "editor/context": [
        {
          "command": "robenkleene.diffGotoSource",
          "when": "editorLangId == 'diff'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  }
}
