# -*- mode: snippet -*-
# key: rk_cout_map
# name: print map
# --
std::cout << "${1} = {";
bool first = true;
for (const auto& pair : values) {
  if (!first) {
    std::cout << ", ";
  }
  std::cout << "{" << pair.first << ": " << pair.second << "}";
  first = false;
}
std::cout << "}" << std::endl;