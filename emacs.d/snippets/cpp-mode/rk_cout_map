# -*- mode: snippet -*-
# key: rk_cout_map
# name: Print map
# --
std::cout << "${1:map} = {";
bool first = true;
for (const auto& pair : values) {
  if (!first) {
    std::cout << ", ";
  }
  std::cout << "{" << pair.first << ": " << pair.second << "}";
  first = false;
}
std::cout << "}" << std::endl;
