alias g='git'
alias ga='git add'
alias gaa='git add -A :/'
alias gaac='git add -A :/ && git commit'
alias gb='git branch'
alias gbum='git branch --set-upstream-to=origin/master master'
alias gbr='git branch -r'
alias gc='git commit'
alias gca='git commit --amend'
alias gcam='git commit --amend -m'
alias gcm='git commit -m'
alias gcl='git clone'
alias gcs='git clone --depth 1'
alias gcn='git clean'
alias gcp='git cherry-pick'
alias gcpc='git cherry-pick --continue'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gd='git diff'
alias gdr='git diff --relative'
alias gdt='git difftool'
alias gdv='git diff --relative | diff-to-grep | vg'
alias gdw='git diff --color-words'
alias gdfu='git diff --name-only --diff-filter=U'
alias gf='git fetch'
alias gl='git log'
alias gm='git merge'
alias gms='git merge --squash'
alias gmt='git mergetool'
alias gp='git push'
alias gpt='git push --tags'
alias gr='git rebase'
alias grc='git rebase --continue'
alias grs='git rebase --skip'
alias grph='git rev-parse HEAD'
alias grphc="git rev-parse HEAD | tr -d '\n' | c"
alias gs='git status'
alias gsm='git submodule'
alias gsmi='git submodule init'
alias gsmu='git submodule update'
alias gsmuir='git submodule update --init --recursive'
alias gsmuirr='git submodule update --init --recursive --remote'
alias gst='git stash'
alias gstl='git stash list'
alias gstp='git stash pop'
alias gt='git tag'
alias gu='git pull'
alias gur='git pull -r'
alias glg='git log -p -G'
